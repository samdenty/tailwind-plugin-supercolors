{
  "name": "tailwind-plugin-supercolors",
  "version": "1.2.2",
  "description": "Tailwind plugin that adds a super- prefix to color utilities",
  "main": "dist/index.js",
  "sideEffects": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist > /dev/null 2>&1; tsc -b",
    "prepublishOnly": "pnpm build",
    "lint": "biome check",
    "lint:fix": "pnpm lint --write"
  },
  "keywords": [
    "tailwindcss",
    "tailwind",
    "tailwind-plugin",
    "colors"
  ],
  "author": "Sam Denty <samddenty@gmail.com> (http://samdenty.com/)",
  "license": "MIT",
  "packageManager": "pnpm@10.7.1",
  "peerDependencies": {
    "tailwindcss": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samdenty/tailwind-plugin-supercolors.git"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "@tailwindcss/vite": "^4.1.4",
    "vite": "^6.3.2"
  }
}
